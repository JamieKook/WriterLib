<div class= "container allBooks">
    {{#each book}}
    <div class = "container book">
        <h3>{{title}}</h3>
        <img src="{{imageURL}}">
        <uL>
            <li>Genre: {{genre}}</li>
            <li>Type:  {{type}}</li>
            <li>Description: {{description}}</li>
        </uL>
        <button data-bookid="{{id}}">Download</button>
    </div>
    {{/each}}
</div>